 <div class="panel-body">
    <div class="media">
      <div class="media-body">
        <table class="table">
<thead><h3 class="survey-title-id" name="survey-title-name" id="{{survey._id}}"> {{survey.title}} </h3></thead>
        <tbody><div>
          <td>{{#each survey.questions}}
            <tr><p id="{{_id}}">{{query}}</p></tr>
            {{#each answers}}
              <tr><div class="radio">
                <label>
                  <input class="radio-button" type="radio" id="{{_id}}" name="{{../_id}}" value="option"> {{text}}
                </label>
              </div></tr>
            {{/each}}
          {{/each}}</td>
        </div></tbody>
      </table>
        <button class="btn btn-success" id="submit-votes" type="submit" name="submit" value="submit">Submit!</button>
      </div>
    </div>
  </div>
